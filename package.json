{
  "name": "munch-twitter-clone",
  "version": "1.0.0",
  "author": "Neill Landman<neilllandman@gmail.com>",
  "license": "NONE",
  "description": "Simple twitter clone built with Fastify, TypeScript, and PostgreSQL",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./dist && npx --yes tsc -p tsconfig.build.json",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "start": "node dist/main.js",
    "start:dev": "nodemon --signal SIGINT | pino-pretty",
    "test": "jest",
    "sequelize-dev": "sequelize-cli --config src/database/config/config.js",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:dev": "npm run build && npm run db:migrate",
    "db:seed:all": "npm run build && npm run db:seed:all"
  },
  "dependencies": {
    "@fastify/jwt": "^9.1.0",
    "bcryptjs": "^3.0.2",
    "dotenv": "^16.4.7",
    "fastify": "^5.2.2",
    "pg": "^8.14.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.23.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.17",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  }
}
